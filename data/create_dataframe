import pandas as pd


def create_dataframe():
    # Sample DataFrame - replace this with your actual data loading logic
    df = pd.DataFrame({
        'Date': ['2021-01-01', '2021-01-02', '2021-01-03'],
        'Sales': [100, 200, 150],
        'Profit': [10, 20, 15]
    })

    # Convert 'Date' to datetime format if needed
    df['Date'] = pd.to_datetime(df['Date'])

    return df
